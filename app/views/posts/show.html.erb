<table class="table table-hover">
  <tbody>
    <tr>
      <td><%= @post.user.name %></td>
    </tr>  
    <tr>
      <td><%= @post.content %></td>
    </tr>
    <tr>
      <td><%= video_tag @post.video_url.to_s,loop: false, controls: true, width: 550, id: "mv" %></td>
    </tr>
    <tr>
      <td><%= link_to "削除",@post,method: :delete%></td>
    </tr>
    <tr>
      <td><%= link_to "編集",edit_post_path%></td>
    </tr>    
  </tbody>
</table>
